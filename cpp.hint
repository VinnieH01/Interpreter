#define ASTLiteralNode(T, name) class AST##name##: public ASTNode { public: AST##name(T value) : m_value(value) {} inline int get_value() const { return m_value; } inline virtual void print() const override { std::cout << "{Literal: " << m_value << "}"; } inline virtual InterpreterResult accept(ASTVisitor<InterpreterResult>& visitor) const { return visitor.visit(*this); } private: T m_value; };
